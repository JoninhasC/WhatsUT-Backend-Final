{
  "name": "whatsut-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "dev:clean": "npm run clean:cache && vite --host",
    "dev:force": "vite --host --force",
    "build": "tsc && vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview --host",
    "clean:cache": "if exist node_modules\\.vite rmdir /s /q node_modules\\.vite && if exist dist rmdir /s /q dist",
    "clean:all": "if exist node_modules rmdir /s /q node_modules && npm install",
    "type-check": "tsc --noEmit",
    "health": "curl -f http://localhost:5173 || exit 1"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.0",
    "axios": "^1.6.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.300.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.0",
    "react-hot-toast": "^2.4.0",
    "react-router-dom": "^6.20.0",
    "socket.io-client": "^4.7.0",
    "tailwind-merge": "^2.0.0",
    "zod": "^3.22.0",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  }
}
